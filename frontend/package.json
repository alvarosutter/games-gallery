  "description": "A collection of simple mini-games.",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --parser-options={tsconfigRootDir:null}",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,yml,css}\" --check",
    "prettier:fix": "npm run prettier -- --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarosutter/games-gallery.git"
  },
  "author": "Alvaro Sutter",
